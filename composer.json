{
    "name": "upkareno/error-reporter",
    "description": "Simple Error Reporting package",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "framework",
        "laravel"
    ],
    "homepage": "https://github.com/upkareno/error-reporter",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/upkareno/error-reporter"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Upkareno\\ErrorReporter\\Providers\\ReporterProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Mohamed Mostafa",
            "email": "mohamed1999250@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Upkareno\\ErrorReporter\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.8 || ^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    }
}